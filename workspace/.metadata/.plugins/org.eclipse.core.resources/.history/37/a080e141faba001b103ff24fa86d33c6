		// TODO Auto-generated method stub
		Course course = new Course();
		course.setCname("abc");
		return course;
	}

	@Override
	public Class getObjectType() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public boolean isSingleton() {
		return false;
	}
}
